node {
    stage('Checkout SCM'){
        git branch: 'master', url: 'https://github.com/eldersol/sa_pra1.git'
    }

    stage('Install node modules'){
        bat 'npm install'
    }
	
	stage('Test'){
        bat 'npm run test:ci'
    }
	
    stage('Build'){
        "build": "ng build --prod",
        bat 'npm run build'
    }
}
